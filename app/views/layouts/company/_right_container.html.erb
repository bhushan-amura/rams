<div class="col-sm-2 well">
  <p>Recent or Upcoming Events:</p>
  <% @company.recent_events(5).each do |e| %>
  <%#= link_to  %>
  <div class="well">
    <%= e.event_type %><br>
    <%= e.date_time.strftime("%b, %d - %Y") %><br/>
    <%= e.date_time.strftime("%I:%M %p") %>
    <%= e.location.city %><br/>
  </div>
  <% end %>
</div>
