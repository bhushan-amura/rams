<p id="notice"><%= notice %></p>

<h1>Listing Company Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_events.each do |company_event| %>
      <tr>
        <td><%= link_to 'Show', company_event %></td>
        <td><%= link_to 'Edit', edit_company_event_path(company_event) %></td>
        <td><%= link_to 'Destroy', company_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_company_event_path %>
